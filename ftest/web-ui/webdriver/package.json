{
  "name": "nuxeo-web-ui-webdriverio",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint test",
    "test": "nuxeo-web-ui-ftest --report --screenshots",
    "pretest": "npm run lint",
    "test:dev": "nuxeo-web-ui-ftest --nuxeoUrl=http://localhost:8080/nuxeo",
    "pretest:dev": "npm run lint",
    "test:watch": "nuxeo-web-ui-ftest --nuxeoUrl=http://localhost:8080/nuxeo --tags=@watch",
    "pretest:watch": "npm run lint"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@nuxeo/nuxeo-web-ui-ftest": "file:../../../nuxeo-web-ui-ftest",
    "nuxeo": "~1.1.1",
    "selenium-standalone": "~5.9.1"
  }
}
