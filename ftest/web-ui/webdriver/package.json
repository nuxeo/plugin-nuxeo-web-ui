{
  "name": "nuxeo-web-ui-webdriverio",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.23.0 <11.0.0"
  },
  "scripts": {
    "postinstall": "check-engine --ignore",
    "test": "nuxeo-web-ui-ftest --report --screenshots --headless",
    "test-not-headless": "nuxeo-web-ui-ftest --report --screenshots",
    "test:dev": "nuxeo-web-ui-ftest --nuxeoUrl=http://localhost:8080/nuxeo --url=http://localhost:5000/",
    "test:watch": "nuxeo-web-ui-ftest --nuxeoUrl=http://localhost:8080/nuxeo  --url=http://localhost:5000/ --tags=@watch --debug"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "check-engine": "^1.10.0",
    "@nuxeo/nuxeo-web-ui-ftest": "file:../../../nuxeo-web-ui-ftest"
  }
}
