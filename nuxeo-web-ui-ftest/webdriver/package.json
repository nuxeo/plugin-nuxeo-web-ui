{
  "name": "nuxeo-web-ui-webdriver",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint test",
    "test": "webui-ftest --report --screenshots",
    "pretest": "npm run lint",
    "test:dev": "webui-ftest --nuxeoUrl=http://localhost:8080/nuxeo",
    "pretest:dev": "npm run lint",
    "test:watch": "webui-ftest --nuxeoUrl=http://localhost:8080/nuxeo --watch --tags=@watch",
    "pretest:watch": "npm run lint"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "nuxeo-web-ui-ftest-framework": "1.1.3-SNAPSHOT",
    "nuxeo": "~1.1.1",
    "selenium-standalone": "~5.9.1"
  }
}
